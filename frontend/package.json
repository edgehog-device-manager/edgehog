{
  "name": "frontend",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.0",
    "@craco/craco": "^6.4.0",
    "@formatjs/cli": "^4.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@hookform/resolvers": "^2.8.3",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/js-cookie": "^3.0.1",
    "@types/leaflet": "^1.7.5",
    "@types/lodash": "^4.14.176",
    "@types/node": "^12.20.36",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-relay": "^13.0.1",
    "@types/react-table": "^7.7.7",
    "@types/relay-runtime": "^13.0.2",
    "@types/relay-test-utils": "^6.0.5",
    "babel-plugin-relay": "^12.0.0",
    "bootstrap": "^5.1.3",
    "dayjs": "^1.10.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-relay": "^1.8.3",
    "graphql": "^15.7.2",
    "history": "^5.1.0",
    "js-cookie": "^3.0.1",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.21",
    "node-sass": "^6.0.1",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.1",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.19.1",
    "react-intl": "^5.21.0",
    "react-leaflet": "^3.2.2",
    "react-relay": "^13.1.1",
    "react-router-dom": "^6.0.0-beta.8",
    "react-scripts": "4.0.3",
    "react-table": "^7.7.0",
    "relay-compiler": "^13.1.1",
    "relay-compiler-language-typescript": "^15.0.1",
    "relay-runtime": "^13.1.1",
    "relay-test-utils": "^13.1.1",
    "typescript": "^4.4.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "init": "npm run relay:compile && npm run i18n:compile",
    "start": "craco start",
    "prestart": "npm run init",
    "build": "craco build",
    "prebuild": "npm run init",
    "relay:generate-schema": "cd ../backend && mix absinthe.schema.sdl --schema EdgehogWeb.Schema ../frontend/src/api/schema.graphql",
    "relay:compile": "relay-compiler",
    "format": "prettier --write src",
    "check-format": "prettier --check src",
    "lint": "eslint --fix src",
    "check-lint": "eslint src",
    "check-types": "tsc --noEmit",
    "precheck-types": "npm run init",
    "jest:test": "TZ=UTC craco test",
    "prejest:test": "npm run init",
    "i18n:extract": "formatjs extract 'src/**/*.ts*' --ignore '**/*.d.ts' --out-file translations.generated.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "i18n:compile": "formatjs compile-folder src/i18n/langs --ast src/i18n/langs-compiled",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:relay/recommended",
      "prettier"
    ],
    "plugins": [
      "relay"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "relay": {
    "artifactDirectory": "./src/api/__generated__",
    "customScalars": {
      "DateTime": "string",
      "Upload": "File"
    },
    "language": "typescript",
    "schema": "./src/api/schema.graphql",
    "src": "./src"
  },
  "proxy": "http://localhost:4000/"
}
